 Color_Shaman = 2459FF
 Color_Warlock = 9482CA
 Color_1 = ff0000
 Color_2 = 00ff00
 Color_3 = 0000ff
 Color_4 = ff0000
 Color_5 = 00ff00
# Data = selected rows and cols of a datasource
 DataSize = 0,2
 Axes_Types = x,y
# Axes_Labels_0 = selected rows from a datasource
